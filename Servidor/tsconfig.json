{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react",
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "*": ["*"]
    },
    "resolveJsonModule": true // Adicionar esta linha
  },
  "include": [
    "./src/*.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
